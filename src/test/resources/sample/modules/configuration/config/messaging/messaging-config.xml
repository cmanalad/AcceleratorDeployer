<?xml version="1.0"?>
<messaging-config
        xmlns="http://guidewire.com/messaging-config">
    <!-- ==============================================================================
    Messaging Configuration
   ============================================================================== -->
    <messaging>
        <!-- Locally added destinations -->
        <destination
                id="1"
                name="CUE"
                numsenderthreads="3"
                requestplugin="CueMessageRequest"
                transportplugin="CueTransportPlugin">
            <event
                    name="ClaimAdded"/>
            <event
                    name="ClaimChanged"/>
            <event
                    name="PolicyChanged"/>
            <event
                    name="PaymentStatusChanged"/>
            <event
                    name="ClaimContactAdded"/>
            <event
                    name="ClaimContactChanged"/>
            <event
                    name="ClaimContactRemoved"/>
            <event
                    name="ClaimContactContactChanged"/>
        </destination>
        <destination
                id="2"
                initialretryinterval="10000"
                maxretries="5"
                name="Davies BVU"
                numsenderthreads="5"
                pollinterval="2000"
                requestplugin="SupplierInstructionRequestPlugin"
                retrybackoffmultiplier="1"
                transportplugin="supplierInstructionTransport">
            <event
                    name="InstructionUpdatesToSupplier_ExtChanged"/>
            <event
                    name="InstructionUpdatesToSupplier_ExtAdded"/>
        </destination>
        <destination
                id="3"
                initialretryinterval="10000"
                maxretries="5"
                name="Davies DCLA"
                numsenderthreads="5"
                pollinterval="2000"
                requestplugin="SupplierInstructionRequestPlugin"
                retrybackoffmultiplier="1"
                transportplugin="supplierInstructionTransport">
            <event
                    name="InstructionUpdatesToSupplier_ExtChanged"/>
            <event
                    name="InstructionUpdatesToSupplier_ExtAdded"/>
        </destination>
        <destination
                id="4"
                name="Auda"
                numsenderthreads="5"
                transportplugin="AudaTransport">
            <event
                    name="ClaimEntityNotificationsCreated"/>
        </destination>
        <destination
                id="5"
                maxretries="1"
                name="ProduceDocument"
                requestplugin="ProduceDocumentRequest"
                transportplugin="ProduceDocumentTransport">
            <event
                    name="produceDocument"/>
        </destination>
        <destination
                id="6"
                name="PolicyDiaryCpd"
                transportplugin="PolicyDiaryCpdTransport">
            <event
                    name="SendCpdPolicyDiary"/>
        </destination>
        <destination
                id="7"
                name="PolicyDiaryChoice"
                transportplugin="PolicyDiaryChoiceTransport">
            <event
                    name="SendChoicePolicyDiary"/>
        </destination>
        <destination
                id="8"
                name="MISurge"
                transportplugin="MiSurgeTransport">
            <event
                    name="ClaimAdded"/>
            <event
                    name="ClaimChanged"/>
        </destination>
        <destination
                id="9"
                maxretries="5"
                name="OracleFinancials"
                numsenderthreads="5"
                pollinterval="2000"
                requestplugin="FinancialsRequest"
                retrybackoffmultiplier="1"
                transportplugin="OracleFinancialsTransport">
            <event
                    name="CheckStatusChanged"/>
            <event
                    name="ReserveStatusChanged"/>
            <event
                    name="PaymentStatusChanged"/>
            <event
                    name="RecoveryStatusChanged"/>
            <event
                    name="RecoveryReserveStatusChanged"/>
        </destination>
        <destination
                id="10"
                name="Admin Audit"
                transportplugin="adminAuditTransport">
            <event
                    name="UserChanged"/>
            <event
                    name="UserAdded"/>
            <event
                    name="UserRemoved"/>
            <event
                    name="AuthorityLimitProfileAdded"/>
            <event
                    name="AuthorityLimitProfileChanged"/>
            <event
                    name="AuthorityLimitProfileRemoved"/>
            <event
                    name="RoleAdded"/>
            <event
                    name="RoleChanged"/>
            <event
                    name="RoleRemoved"/>
            <event
                    name="GroupAdded"/>
            <event
                    name="GroupChanged"/>
            <event
                    name="GroupRemoved"/>
            <event
                    name="ContactChanged"/>
        </destination>
        <destination
                id="11"
                name="Credit Hire Instruction"
                transportplugin="CreditHireTransport">
            <event
                    name="ClaimEntityNotificationsCreated"/>
        </destination>
        <destination
                id="12"
                name="Repairer Instruction"
                transportplugin="RepairInstructionTransport">
            <event
                    name="ClaimEntityNotificationsCreated"/>
        </destination>
        <destination
                id="13"
                name="NonOracleFinancials"
                transportplugin="NonOracleFinancialsTransport">
            <event
                    name="CheckStatusChanged"/>
        </destination>
        <destination
                id="14"
                name="PolicyUnderwritingUpdateCPD"
                transportplugin="PolicyUnderwritingCPD">
            <event
                    name="SendPolicyUnderwritingUpdate"/>
            <event
                    name="ClaimChanged"/>
            <event
                    name="PolicyChanged"/>
            <event
                    name="TransactionChanged"/>
            <event
                    name="TransactionRemoved"/>
            <event
                    name="TransactionAdded"/>
            <event
                    name="ExposureChanged"/>
            <event
                    name="ExposureAdded"/>
            <event
                    name="PaymentStatusChanged"/>
            <event
                    name="ReserveStatusChanged"/>
            <event
                    name="RecoveryReserveStatusChanged"/>
            <event
                    name="PaymentChanged"/>
            <event
                    name="VehicleIncidentAdded"/>
            <event
                    name="VehicleIncidentRemoved"/>
            <event
                    name="VehicleIncidentChanged"/>
            <event
                    name="LVAssociationChanged"/>
        </destination>
        <destination
                id="15"
                name="PolicyUnderwritingUpdateChoice"
                transportplugin="PolicyUnderwritingChoice">
            <event
                    name="SendPolicyUnderwritingUpdate"/>
            <event
                    name="ClaimChanged"/>
            <event
                    name="PolicyChanged"/>
            <event
                    name="TransactionChanged"/>
            <event
                    name="TransactionRemoved"/>
            <event
                    name="TransactionAdded"/>
            <event
                    name="ExposureChanged"/>
            <event
                    name="ExposureAdded"/>
            <event
                    name="PaymentStatusChanged"/>
            <event
                    name="ReserveStatusChanged"/>
            <event
                    name="RecoveryReserveStatusChanged"/>
            <event
                    name="PaymentChanged"/>
            <event
                    name="VehicleIncidentAdded"/>
            <event
                    name="VehicleIncidentRemoved"/>
            <event
                    name="VehicleIncidentChanged"/>
            <event
                    name="LVAssociationChanged"/>
        </destination>
        <destination
                id="16"
                initialretryinterval="10000"
                maxretries="5"
                name="DocumentSupplierInstructions"
                numsenderthreads="3"
                pollinterval="2000"
                retrybackoffmultiplier="1"
                transportplugin="DocumentSupplierInstructionTransport">
            <event
                    name="InstructionUpdatesToSupplier_ExtChanged"/>
            <event
                    name="InstructionUpdatesToSupplier_ExtAdded"/>
        </destination>
        <destination
                id="17"
                name="MIAFTR"
                numsenderthreads="5"
                requestplugin="MiaftrMessageRequestPlugin"
                retrybackoffmultiplier="1"
                transportplugin="MiaftrTransport">
            <event
                    name="ClaimEntityNotificationsCreated"/>
        </destination>
        <destination
                id="18"
                name="MidEnquiry"
                transportplugin="MidEnquiryTransport">
            <event
                    name="ClaimEntityNotificationsCreated"/>
        </destination>
        <destination
                id="19"
                name="IrwinMitchell"
                transportplugin="IrwinMitchellTransport">
            <event
                    name="ClaimEntityNotificationsCreated"/>
        </destination>
        <!-- OOTB Destinations  Email Messaging destination -->
        <destination
                disabled="true"
                id="65"
                initialretryinterval="500"
                internal="true"
                maxretries="5"
                name="email"
                transportplugin="emailMessageTransport"/>
        <!-- ISO destination.
DO NOT CHANGE THE id OF THIS DESTINATION
Enable to enable sending messages to the ISO ClaimSearch serviceco -->
        <destination
                disabled="true"
                id="66"
                initialretryinterval="500"
                internal="true"
                maxretries="5"
                name="iso"
                replyplugin="IISOReplyPlugin"
                transportplugin="isoTransport">
            <event
                    name="ExposureAdded"/>
            <event
                    name="ExposureValid"/>
            <event
                    name="ExposureChanged"/>
            <event
                    name="ClaimValid"/>
            <event
                    name="ClaimChanged"/>
            <event
                    name="ClaimResync"/>
            <event
                    name="PolicyChanged"/>
            <event
                    name="ClaimContactContactChanged"/>
            <event
                    name="ClaimContactRoleAdded"/>
            <event
                    name="ClaimContactRoleChanged"/>
            <event
                    name="ClaimContactRoleRemoved"/>
        </destination>
        <destination
                disabled="true"
                id="67"
                internal="true"
                maxretries="5"
                name="metro"
                requestplugin="metroRequest"
                transportplugin="metroTransport">
            <event
                    name="MetroReportAdded"/>
            <event
                    name="MetroReportChanged"/>
        </destination>
        <!-- destination for example console transport
    Enable to print out a message for every event.  Make sure you also enable the <plugin-java> element
    for the "consoleTransport" plugin. -->
        <destination
                disabled="true"
                id="68"
                initialretryinterval="100"
                internal="true"
                name="Console Message Logger"
                transportplugin="consoleTransport">
            <event
                    name="\w*"/>
        </destination>
        <destination
                disabled="true"
                id="69"
                internal="true"
                name="Policy System Notification"
                transportplugin="policySystemNotificationTransport">
            <event
                    name="ClaimExceedsLargeLoss"/>
            <event
                    name="ClaimResync"/>
        </destination>
        <!-- Contact Auto Sync failure notifier. This event destination notifies the admin about
any contact auto sync failure. Customers can write their own transport plugin to do other
things upon the auto sync failure.
DO NOT CHANGE THE id OF THIS DESTINATION
(If the id must be changed, change the FireEvent rule in Studio as well) -->
        <destination
                id="80"
                initialretryinterval="100"
                internal="true"
                name="Contact Auto Sync Failure"
                transportplugin="autoSyncFailureTransport">
            <event
                    name="ContactAutoSyncFailed"/>
        </destination>
        <!-- destination for JMS Messaging Example.  Make sure you also enable the <plugin-java> element -->
        <destination
                disabled="true"
                id="111"
                initialretryinterval="100"
                internal="true"
                maxretries="20"
                name="JMSMessageTransport"
                replyplugin="jmsReply"
                transportplugin="jmsTransport">
            <event
                    name="NoteAdded"/>
        </destination>
        <destination
                disabled="true"
                id="324"
                internal="true"
                name="Document Store"
                transportplugin="documentStoreTransport">
            <event
                    name="DocumentStore"/>
        </destination>
    </messaging>
</messaging-config>